{
  "compilerOptions": {
    "noImplicitReturns": true,
    "typeRoots": ["node_modules/@types"],
    "target": "es5",
    "lib": ["dom", "es6", "scripthost"],
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist",
    "skipDefaultLibCheck": true
